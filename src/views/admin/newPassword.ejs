<div class="container-admin">
    <h2>Ange nytt lösenord</h2>
    <form action="/reset-password" method="post" class="admin-login-form">
        <div class="form-group">
            <label for="password">Nytt lösenord:</label>
            <input type="password" id="password" name="password" required>
        </div>
        <div class="form-group">
            <label for="passwordsame">Ange Lösenordet igen:</label>
            <input type="passwordsame" id="passwordsame" name="passwordsame" required>
        </div>
        <div class="form-group">
            <input type="submit" value="Login">
        </div>
    </form>
</div>
